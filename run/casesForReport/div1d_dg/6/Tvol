FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "6";
    object      Tvol;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 1 0 0];

internalField   nonuniform List<scalar> 10(4 4 3.999999999 4.000000041 3.999999808 4.000012788 4.000156648 4.002167983 4.037050007 4.267582102);

boundaryField
{
    left
    {
        type            calculated;
        value           uniform 4;
    }
    right
    {
        type            calculated;
        value           uniform 4;
    }
    sides
    {
        type            empty;
    }
}


