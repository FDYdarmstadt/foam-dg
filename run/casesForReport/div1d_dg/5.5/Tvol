FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "5.5";
    object      Tvol;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 1 0 0];

internalField   nonuniform List<scalar> 10(4 4 4.000000014 3.999999971 4.000003508 4.000111872 4.000976805 4.023805456 4.220078379 4.932449647);

boundaryField
{
    left
    {
        type            calculated;
        value           uniform 0;
    }
    right
    {
        type            calculated;
        value           uniform 0;
    }
    sides
    {
        type            empty;
    }
}


