FoamFile
{
    version     2.0;
    format      ascii;
    class       cellVectorField;
    location    "38.9";
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 1 0 0];

internalField   uniform ((1 0 0) (0 0 0) (0 0 0));

boundaryField
{
    left
    {
        type            dirichlet;
        value           uniform ((1 0 0) (0 0 0) (0 0 0));
    }
    right
    {
        type            dirichlet;
        value           uniform ((1 0 0) (0 0 0) (0 0 0));
    }
    sides
    {
        type            empty;
    }
}


