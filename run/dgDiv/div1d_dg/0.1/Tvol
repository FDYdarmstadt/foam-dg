FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0.1";
    object      Tvol;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 1 0 0];

internalField   nonuniform List<scalar> 10(9.466101695 9.986426314 9.999654906 9.999991226 9.999999777 9.999999994 10 10 10 10);

boundaryField
{
    left
    {
        type            calculated;
        value           uniform 0;
    }
    right
    {
        type            calculated;
        value           uniform 0;
    }
    sides
    {
        type            empty;
    }
}


